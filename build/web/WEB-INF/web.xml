<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servlettest</servlet-name>
        <servlet-class>servlettest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Aircontroller</servlet-name>
        <servlet-class>servlettest.Aircontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>util.ImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jip</servlet-name>
        <servlet-class>bean.Jip</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jipiao</servlet-name>
        <servlet-class>servlettest.Jipiao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlettest</servlet-name>
        <url-pattern>/servlettest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Aircontroller</servlet-name>
        <url-pattern>/Aircontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/ImageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jip</servlet-name>
        <url-pattern>/Jip</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jipiao</servlet-name>
        <url-pattern>/Jipiao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
